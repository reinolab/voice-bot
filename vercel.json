{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "DEEPGRAM_API_KEY": "@deepgram_api_key",
    "OPENAI_API_KEY": "@openai_api_key",
    "CARTESIA_API_KEY": "@cartesia_api_key"
  }
} 